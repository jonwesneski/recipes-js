{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.ts$": "ts-jest"
  },
  "moduleNameMapper": {
    "^src/(.*)$": "<rootDir>/../src/$1"
  },
  "globalSetup": "<rootDir>/setup.ts",
  "globalTeardown": "<rootDir>/teardown.ts",
  "verbose": true,
  "silent": false
}
